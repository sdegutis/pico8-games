function _init()
	debug=nil
	starttitle()
end

function startgame()
	music()
	
	_draw=drawgame
	_update=updategame
	
	powers={
		{'speed',30},
		{'fire',50},
		{'bomb',45},
		{'pbomb',20},
		{'sbomb',15},
		{'wbomb',50},
		{'mine',30},
		{'brick',10},
		{'blinkmode',10},
		{'invisible',30},
	}
	
	gameover=nil
	players={}
	bombs={}
	bricks={}
	flames={}
	items={}
	showitems=false
	
	spots = {
		{8,8},
		{104,104},
		{8,104},
		{104,8},
	}
	
	for i=1,numplayers do
		local x = spots[i][1]
		local y = spots[i][2]
		makeplayer(i-1,x,y)
	end
	
	decodemap()
	spawnbricks()
	
	for i=1,#bricks-1 do
		for j=i+1,#bricks do
			local a = bricks[i]
			local b = bricks[j]
			if a.x == b.x and a.y == b.y then
				cls()
				print(a.x)
				print(a.y)
				print(b.x)
				print(b.y)
				stop()
			end
		end
	end
end

function drawgame()
	
	local camx=-4
	local camy=-4
	if shaking then
		camx+=rnd(2)*sgn(rnd(2)-1)
		camy+=rnd(2)*sgn(rnd(2)-1)
	end
	
	camera(camx,camy)
	
	cls(0)
	map(mapx*16,mapy*16,0,0,15,15)
	foreach(bricks,drawbrick)
	foreach(bombs,drawbomb)
	foreach(flames,drawflame)
	foreach(items,drawitem)
	
	if gameover then
		if gameover.winner then
			local p = gameover.winner
			local x1=p.x-2
			local y1=p.y-2
			local x2=p.x+10
			local y2=p.y+10
			rectfill(x1,y1,x2,y2,10)
			rect(x1,y1,x2,y2,9)
			box(p.x-12,p.y-13,"winner!")
		else
			box(40,40,"no winner")
		end
		
		if gameover.t == 0 then
			box(23,60,"press (x) to start")
		end
	end
	
	foreach(players,drawplayer)
	
	if debug then
	 if debug.pset then
	 	pset(debug.x1, debug.y1, 11)
	 	pset(debug.x2, debug.y2, 11)
	 else
		 local y=1
	 	for k,v in pairs(debug) do
	 	 local s = tostr(k)..'='..tostr(v)
	 	 rectfill(0,y-1,#s*4,y+5,1)
		 	print(s,1,y,7)
		 	y += 8
		 end
	 end
	end
	camera()
end

function updategame()
	if shaking then
		shaking -= 1
		if shaking == 0 then
			shaking=nil
		end
	end

	for k,v in pairs(cheats) do
		for i=1,#players do
		 local p=players[i]
			checkcheat(p,k,v)
		end
	end
	
	if gameover then
		if gameover.t > 0 then
			gameover.t -= 1
		else
			if btnp(‚ùé) then
				choosemap()
				return
			end
		end
	end
	
	foreach(players,updateplayer)
	foreach(bombs,updatebomb)
	foreach(flames,updateflame)
end

function checkgameover()
	local live = {}
	for i = 1,#players do
		local p = players[i]
		if (not p.out) add(live,p)
	end
	if #live == 1 then
		music(-1)
		sfx(5)
		gameover = {
			t=90,
			winner=live[1],
		}
	elseif #live == 0 then
		music(-1)
		sfx(6)
		gameover = {
			t=90,
		}
	end
end

function checkcheat(p,name,codes)
 -- initial state
 local c = p[name]
 if not c then
 	c = {}
		c.t = 0
		c.i = 0
 	p[name] = c
 end
 
	-- count down if active
	if c.i > 0 then
		c.t -= 1
		-- reset if times up
		if c.t == 0 then
			c.i = 0
		end
	end
	
	-- is current button pressed?
	if btnp(codes[c.i+1],p.n) then
		c.i += 1
		c.t = 15 -- time limit
		if c.i == #codes-1 then
			local fn = codes[#codes]
			-- finished!
			fn(p)
			c.i = 0
			c.t = 0
		end
	end
end

