pico-8 cartridge // http://www.pico-8.com
version 42
__lua__


function _init()
	ents={}

	cx=0
	cy=0

	for y=0,127 do
		for x=0,63 do
			local s = mget(x,y)
			if fget(s, 0) then
				-- solid
				makesolid(s,x*8,y*8)
				mset(x,y,1)
			elseif fget(s, 3) then
				makeplayer(s,x*8,y*8)
				mset(x,y,1)
			end
		end
	end
end

function _update()
	for e in all(ents) do
		if (e.update) e:update()
	end
end

function _draw()
	cls()
	camera(cx,cy)
	map()
	for e in all(ents) do
		if (e.draw) e:draw()
	end
end

function makeplayer(s,x,y)
	add(ents, {
		s=s,x=x,y=y,
		draw=drawplayer,
		update=updateplayer,
	})
end

function makesolid(s,x,y)
	add(ents, {
		s=s,x=x,y=y,
		draw=drawsimple,
	})
end

function drawplayer(p)
	spr(p.s, p.x, p.y)
end

function drawsimple(e)
	spr(e.s, e.x, e.y)
end

function updateplayer(p)
	if (btn(â¡ï¸)) trymove(p, 1, 0)
	if (btn(â¬…ï¸)) trymove(p, -1, 0)

	p.grounded = trymove(p, 0, 1)
end

function trymove(e,x,y)
	e.x += x
	e.y += y

	for o in all(ents) do
		if o != e and e.x >= o.x - 7
		          and e.y >= o.y - 7
		          and e.x < o.x + 7
		          and e.y < o.y + 7
		then
			e.x -= x
			e.y -= y
			return false
		end
	end

	return true
end

